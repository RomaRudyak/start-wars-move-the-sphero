<Window x:Class="SWMS.WPF.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" Height="350" Width="525">
    <Grid>
        <Grid.RowDefinitions>
            <RowDefinition/>
            <RowDefinition Height="80"/>
        </Grid.RowDefinitions>
        <Grid.ColumnDefinitions>
            <ColumnDefinition/>
            <ColumnDefinition/>
        </Grid.ColumnDefinitions>
        <Viewbox Grid.Row="0" Grid.ColumnSpan="2" HorizontalAlignment="Center">
            <Image x:Name="Image" Stretch="UniformToFill" />
        </Viewbox>
        <TextBlock Grid.Row="1" Grid.Column="0" x:Name="Out">
            
        </TextBlock>
        <Button Grid.Row="1" Grid.Column="1" Content="Clear" Click="Button_Click"/>
    </Grid>
</Window>
